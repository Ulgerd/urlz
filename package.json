{
  "name": "urlz",
  "description": "Functional utils for URLs",
  "version": "0.1.0",
  "author": "Ivan Kleshnin <ivan@paqmind.com> (paqmind.com)",
  "license": "MIT",
  "bugs": "https://github.com/ivan-kleshnin/urlz/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivan-kleshnin/urlz"
  },
  "keywords": [
    "url",
    "path",
    "query",
    "functional",
    "fp"
